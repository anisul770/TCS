{
    "builds": [{
      "src": "transparent_cleaning_service/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.3" }
    }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "transparent_cleaning_service/wsgi.py"
      }
    ]
}